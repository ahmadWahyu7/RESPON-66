{
    "env": {
      "browser": true,
      "es2021": true,
      "node": true
    },
    "extends": ["airbnb-base"],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "rules": {
      "prefer-arrow-callback": "off",
      "prefer-const": "off",
      "func-names": "off",
      "dot-notation": "off",
      "class-methods-use-this": "off",
      "no-unused-vars": "off",
      "max-len": "off",
      "no-trailing-spaces": "off",
      "no-console": "off",
      "comma-dangle": "off",
      "prefer-regex-literals": "off",
      "linebreak-style": "off",
      "no-underscore-dangle": "off",
      "operator-linebreak": "off",
      "no-prototype-builtins": "off",
      "no-undef": "off",
      "no-return-assign": "off",
      "import/prefer-default-export": "off",
      "import/order": "off",
      "consistent-return": "off",
      "import/no-extraneous-dependencies": "off",
      "eol-last": "off",
      "no-restricted-globals": ["error", "event", "fdescribe"],
      "import/extensions": [
        "error",
        "ignorePackages",
        {
          "js": "never",
          "jsx": "never",
          "ts": "never",
          "tsx": "never"
        }
      ]
    }
  }
  
  